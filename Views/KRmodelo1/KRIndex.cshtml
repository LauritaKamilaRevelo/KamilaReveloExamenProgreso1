@model IEnumerable<KamilaReveloExamenProgreso1.Models.KRmodelo1>

@{
    ViewData["Title"] = "Pagina Inical";
}

<h1>Pagina Inicial</h1>

<p>
    <a asp-action="KRCreate">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WithTea)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Precio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaRegristo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaFinal)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WithTea)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Precio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaRegristo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaFinal)
            </td>
            <td>
                <a asp-action="KREdit" asp-route-id="@item.Id">KREdit</a> |
                <a asp-action="KRDetails" asp-route-id="@item.Id">KRDetails</a> |
                <a asp-action="KRDelete" asp-route-id="@item.Id">KRDelete</a>
            </td>
        </tr>
}
    </tbody>
</table>
